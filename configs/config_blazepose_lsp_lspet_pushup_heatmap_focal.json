{
    "experiment_name":                  "blazepose_lsp_lspet_pushup_heatmap_focal",
    "trainer":                          "blazepose_trainer",
    "data_loader":                      "humanpose",      
    "data": {   
        "train_images":                 "data/lsp_lspet_pushup/images/",
        "train_labels":                 "data/lsp_lspet_pushup/train.json",
        "val_images":                   "data/lsp_lspet_pushup/images/",
        "val_labels":                   "data/lsp_lspet_pushup/val.json",
        "test_images":                  "data/lsp_lspet_pushup/images/",
        "test_labels":                  "data/lsp_lspet_pushup/test.json",
        "symmetry_point_ids":           [[0,6], [1,5], [2,4]]
    },                   
    "model" : {
        "im_width":                     256,
        "im_height":                    256,
        "heatmap_width":                128,
        "heatmap_height":               128,
        "heatmap_kp_sigma":             4,
        "num_keypoints":                7,
        "model_type":                  "TWO_HEAD"
    },
    "train": {
        "train_phase":                  "HEATMAP",
        "heatmap_loss":                 "focal",
        "keypoint_loss":                "mean_squared_error",
        "loss_weights":                 {"heatmap": 1.0, "joints": 0.0},
        "train_batch_size":             8,
        "val_batch_size":               8,
        "nb_epochs":                    1000,
        "learning_rate":                1e-4,
        "load_weights":                 false,
        "pretrained_weights_path":      "",
        "initial_epoch":                0
    },
    "test": {
        "pck_ref_points_idxs" :         [2, 4],
        "pck_thresh":                   0.2
    }
}
